<%= form_for :dump, :url=>{:action=>"upload_csv"}, :html => { :multipart => true } do |f| %>
  <table>
    <tr>
      <td>
        <label for = "dump_file">
          <%= f.file_field :file %>
        </label>
      </td>
    </tr>
    <tr>
      <td>
        <%= submit_tag 'Submit' %>
      </td>
    </tr>
  </table>  
<% end %>